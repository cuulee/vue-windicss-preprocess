{
  "name": "vue-windicss-preprocess",
  "version": "2.0.0",
  "description": "A vue loader to compile tailwindcss at build time based on windicss compiler.",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/voorjaar/vue-windicss-preprocess.git"
  },
  "keywords": [
    "vue",
    "vue-loader",
    "tailwindcss",
    "css",
    "compiler"
  ],
  "author": "Veritas Raven",
  "license": "MIT",
  "dependencies": {
    "loader-utils": "^2.0.0",
    "magic-string": "^0.25.7",
    "windicss": "^2.1.3"
  },
  "devDependencies": {
    "@types/loader-utils": "^2.0.1",
    "@types/node": "^14.14.14",
    "typescript": "^4.1.3"
  },
  "files": [
    "src/**/*.js",
    "src/**/*.d.ts"
  ]
}
